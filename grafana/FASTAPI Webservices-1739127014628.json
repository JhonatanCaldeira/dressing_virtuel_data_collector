          "rawSql": "SELECT \r\n\tcount(*) as cnt_error, \r\n\tregexp_substr(message, '\\s([\\w.]+)\\s', 1, 1) AS extracted_api,\r\n\t$__timeGroupAlias(\"created_at\",5m)\r\nFROM \r\n\tlogs \r\nWHERE \"level\" = 'ERROR' and message like '%api.%' and $__timeFilter(created_at)\r\ngroup BY time, extracted_api\r\nORDER BY time",
